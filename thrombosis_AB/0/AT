FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      AT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 2.844e-3;

boundaryField
{
    inlet
    {
        type            fixedValue;
	value		uniform 2.844e-3;
    }

    outlet
    {
        type            zeroGradient;
    }

    upperWall
    {
        type            zeroGradient;
    }

    lowerWall
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

